{
  "$schema": "http://schemas.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "cosmosDb": {
      "type": "object",
      "defaultValue": {
        "resourceGroupName": "tompostler",
        "accountName": "tompostler-free",
        "databaseName": "shared",
        "collectionName": "nstcpwtf"
      }
    }
  },
  "resources": [
    {
      "type": "Microsoft.DocumentDB/databaseAccounts/sqlDatabases/containers",
      "apiVersion": "2020-04-01",
      "name": "[concat(parameters('cosmosDb').accountName, '/', parameters('cosmosDb').databaseName, '/', parameters('cosmosDb').collectionName)]",
      "properties": {
        "resource": {
          "id": "[parameters('cosmosDb').collectionName]",
          "partitionKey": {
            "paths": [
              "/PK"
            ]
          }
        },
        "options": {}
      }
    }
  ]
}